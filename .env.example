# Swarm SH Configuration
# Copy to .env and customize

# Language Model Configuration
SWARMSH_LM=gpt-4o-mini
SWARMSH_LM_TEMPERATURE=0.1
SWARMSH_LM_MAX_TOKENS=4096

# Cost Controls
SWARMSH_MAX_COST_PER_DAY=50.00
SWARMSH_TOKEN_BUDGET_WARN=0.8

# OTEL Configuration  
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=swarmsh-5one

# Governance Settings
SWARMSH_QUORUM_THRESHOLD=0.5
SWARMSH_AUTO_APPROVE_LOW_RISK=true

# WASI Runtime
SWARMSH_WASI_MAX_EXECUTION_MS=5
SWARMSH_WASI_MAX_MEMORY_MB=16